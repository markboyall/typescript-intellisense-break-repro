{
  "compilerOptions": {
    "experimentalDecorators": true,
    "module": "amd",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "outFile": "build/Bluewire.Epro.Modern.ServiceAndGeneratedPages.js",
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "allowUnreachableCode": false,
    "noImplicitReturns": true,
    "baseUrl": "./",
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "lib": [ "dom", "es5", "es2015.iterable", "es2015.promise" ],
    "declaration": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": [],
    "typeRoots": [],
    "strictNullChecks": true,
    "declarationMap": true,
    "composite": true
  },
  "compileOnSave": false,
  "include": [
    "obj/**/*.d.ts",
    "App/**/*.ts",
    "Specs/**/*.ts",
    "../Epro.Mobile/Dependencies/jquery.d.ts",
    "../Epro.Mobile/Dependencies/jasmine.d.ts",
    "../Epro.Mobile/Dependencies/JasmineAddons.d.ts",
    "../Epro.Mobile/Dependencies/underscore.d.ts",
    "../Epro.Mobile/Dependencies/moment.d.ts"
  ],
  "references": [
      { "path": "../Bluewire.Epro.Modern.Framework" }
  ]
}
